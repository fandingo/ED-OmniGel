		</ingredients>
		<products>
			<ChunkSlagSteel>1</ChunkSlagSteel>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>OmniGel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
	</RecipeDef>

