<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

	<!-- Replicate Steel -->
	<RecipeDef>
		<defName>Replicate_MK2_SteelFromOmniGel</defName>
		<label>Replicate Steel from Omni-Gel</label>
		<description>Replicates Steel from Omni-Gel.</description>
		<jobString>Creating Steel from Omni-Gel.</jobString>
		<workAmount>500</workAmount>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>OmniGel</li>
					</thingDefs>
				</filter>
				<count>40</count>
			</li>
		</ingredients>
		<products>
			<Steel>20</Steel>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>OmniGel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
	</RecipeDef>

